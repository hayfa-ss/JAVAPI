<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="635.0" prefWidth="659.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.Affichernotificationcontroller">
    <children>
        <ComboBox fx:id="invitationStatusField" layoutX="77.0" layoutY="70.0" prefHeight="31.0" prefWidth="214.0" promptText="Status">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="En attente" />
                    <String fx:value="Annulée" />
                    <String fx:value="Confirmée" />
                </FXCollections>
            </items>
        </ComboBox>

        <ComboBox fx:id="notificationPreferenceField" layoutX="77.0" layoutY="27.0" prefHeight="31.0" prefWidth="214.0" promptText="NotificationPreferences">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Email" />
                    <String fx:value="SMS" />
                    <!-- Ajoutez d'autres options si nécessaire -->
                </FXCollections>
            </items>
        </ComboBox>
        <TextArea fx:id="messageField" layoutX="77.0" layoutY="116.0" prefHeight="147.0" prefWidth="227.0" promptText="Message" />
        <Button layoutX="122.0" layoutY="284.0" mnemonicParsing="false" onAction="#ajouterNotification" text="Ajouter Notification" textFill="#030000" />
      <ImageView fitHeight="659.0" fitWidth="659.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/12.png" />
         </image>
      </ImageView>
        <!-- Autres éléments de l'interface utilisateur -->

        <Button layoutX="554.0" layoutY="604.0" mnemonicParsing="false" onAction="#naviguezVersAjouter" prefHeight="31.0" prefWidth="91.0" style="-fx-background-color: #ffa43d;" text="RETOUR">
         <font>
            <Font name="Arial" size="14.0" />
         </font></Button>
        <TableView fx:id="tableView" layoutY="190.0" prefHeight="394.0" prefWidth="659.0">
            <columns>

                <TableColumn fx:id="IdRColumn" prefWidth="36.0" text="IDR" />
                <TableColumn fx:id="messageColumn" prefWidth="92.0" text="Message" />
                <TableColumn fx:id="notificationPreferenceColumn" prefWidth="174.0" text="Notification Preference" />
                <TableColumn fx:id="invitationStatusColumn" minWidth="0.0" prefWidth="155.0" text="Invitation Status" />
                <TableColumn fx:id="modifyTC" prefWidth="99.0" text="Modifier" />
                <TableColumn fx:id="deleteTC" minWidth="0.0" prefWidth="102.0" text="Supprimer" />
            </columns>

        </TableView>
    </children>

</AnchorPane>
