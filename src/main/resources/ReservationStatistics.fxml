<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.ReservationStatistics">
<children>
        <BarChart  fx:id="barChart" layoutX="10.0" layoutY="10.0" prefHeight="600.0" prefWidth="800.0">
            <xAxis>
                <CategoryAxis fx:id="XAxis" side="BOTTOM" />
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="YAxis" side="LEFT" />
            </yAxis>
        </BarChart>
        <Button layoutX="10.0" layoutY="620.0" mnemonicParsing="false" onAction="#loadStatistics" text="Charger les statistiques" />
    </children>
</AnchorPane>
